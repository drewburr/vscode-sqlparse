{
	"name": "vscode-sqlparse",
	"displayName": "sqlparse for Visual Studio Code",
	"description": "Format SQL Code",
	"version": "1.0.1",
	"publisher": "tinytamb",
	"author": {
		"name": "mz"
	},
	"icon": "images/icon.png",
	"license": "MIT",
	"engines": {
		"vscode": "^1.41.0"
	},
	"bugs": {
		"url": "https://github.com/tinytamb/vscode-sqlparse/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/tinytamb/vscode-sqlparse.git"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onLanguage:sql"
	],
	"capabilities": {
		"documentFormattingProvider": "true"
	},
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "sqlparse",
			"properties": {
				"sqlparse.keywordCase": {
					"type": "string",
					"default": "Upper case",
					"enum": [
						"Unchanged",
						"Upper case",
						"Lower case",
						"Capitalize"
					]
				},
				"sqlparse.identifierCase": {
					"type": "string",
					"default": "Unchanged",
					"enum": [
						"Unchanged",
						"Upper case",
						"Lower case",
						"Capitalize"
					]
				},
				"sqlparse.stripComments": {
					"type": "boolean",
					"default": false,
					"description": "Remove comments"
				},
				"sqlparse.reindent": {
					"type": "boolean",
					"default": true,
					"description": "Auto Indent"
				},
				"sqlparse.indentTabs": {
					"type": "boolean",
					"default": false,
					"description": "Use tabs instead of spaces for indentation"
				},
				"sqlparse.indentWidth": {
					"type": "number",
					"minimum": 1,
					"default": 2,
					"description": "Indentation width"
				},
				"sqlparse.outputFormat": {
					"type": "string",
					"default": "SQL",
					"description": "Programming language",
					"enum": [
						"SQL",
						"Python",
						"PHP"
					]
				},
				"sqlparse.pythonPath": {
					"type": "string",
					"default": "/usr/bin/python3",
					"description": "Recommend: Python version >= 3.4"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"python-shell": "^1.0.8"
	}
}
